[
  {
    "name": "frontend",
    "image": "${account_id}.dkr.ecr.${region}.amazonaws.com/${name}-frontend:${tag}",
    "command": [
      "yarn",
      "start"
    ],
    "portMappings": [
      {
        "containerPort": "${port}",
        "hostPort": "${port}",
        "protocol": "tcp"
      }
    ],
    "managedAgents": [
      {
        "lastStartedAt": "2021-03-01T14:49:44.574000-06:00",
        "name": "ExecuteCommandAgent",
        "lastStatus": "RUNNING"
      }
    ],
    "linuxParameters": {
      "initProcessEnabled": true
    },
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "${region}",
        "awslogs-group": "/${name}/frontend",
        "awslogs-stream-prefix": "${name}-frontend"
      }
    },
    "environment": [
      {
        "name": "PORT",
        "value": 80
      }
    ]
  }
]